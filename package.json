{
  "name": "eyrus-sam-integration",
  "version": "1.0.0",
  "description": "Enterprise-grade SAM.gov opportunities API integration for Eyrus project",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:implementation": "ts-node src/scripts/test-implementation.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "migrate": "knex --knexfile knexfile.js migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "health-check": "ts-node -e \"import('./src/services/healthCheckService').then(h => h.healthCheckService.performHealthCheck(true).then(console.log))\""
  },
  "dependencies": {
    "axios": "^1.6.0",
    "better-sqlite3": "^11.10.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "knex": "^3.1.0",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "rate-limiter-flexible": "^5.0.3",
    "retry-axios": "^3.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "sam.gov",
    "government",
    "opportunities",
    "api",
    "enterprise",
    "eyrus"
  ],
  "author": "Eyrus Team",
  "license": "MIT"
}
